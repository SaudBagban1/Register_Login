<?xml version="1.0" encoding="UTF-8"?>

<web-app>
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>

	<servlet>
		<servlet-name>MyRegister</servlet-name>
		<servlet-class>com.register.Register</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>MyRegister</servlet-name>
		<url-pattern>/registerForm</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>MyLogin</servlet-name>
		<servlet-class>com.login.Login</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>MyLogin</servlet-name>
		<url-pattern>/loginForm</url-pattern>
	</servlet-mapping>

</web-app>
